
{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "es2015"],
    // "lib": [
    //   "dom",
    //   "dom.iterable",
    //   "esnext"
    // ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    // "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "outDir": "./lib/cjs/",
    "sourceMap": true,
    "declaration": true,
    "noImplicitAny": true,
    "module": "commonjs",
  },
  // "include": [
  //   "src"
  // ]
  "include": ["src/**/*.tsx*","src/svg.d.ts"],
  "exclude": ["node_modules", "dist", "lib"]
}
